{
	"printWidth": 140,
	"tabWidth": 3,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"arrowParens": "always",
	"proseWrap": "preserve",
	"endOfLine": "lf",
	"embeddedLanguageFormatting": "auto",
	"plugins": ["prettier-plugin-tailwindcss"]
}
